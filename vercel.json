{
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "recorder.html", "use": "@vercel/static" },
    { "src": "avatar.html", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Permissions-Policy", "value": "microphone=(self), camera=(self)" }
      ]
    }
  ]
}
